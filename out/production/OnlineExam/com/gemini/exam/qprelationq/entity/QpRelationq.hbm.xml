<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
 	<class name="com.gemini.exam.qprelationq.entity.QpRelationq" table="qprelationq">
 		<id name="rid" column="rid">
 			<generator class="native"/>
 		</id>
 		<property name="num" column="num"></property>
 		<many-to-one name="questionPaper" class="com.gemini.exam.questionpaper.entity.QuestionPaper" column="qpid"></many-to-one>
 		<many-to-one name="question" class="com.gemini.exam.question.entity.Question" column="qid"></many-to-one>
 	</class>
 </hibernate-mapping>
